    <defName>LS_SwordMiss</defName>
    <eventNames />
    <context>MapOnly</context>
    <maxSimultaneous>1</maxSimultaneous>
    <subSounds>
      <li>
        <onCamera>True</onCamera>
        <grains>
          <li Class="AudioGrain_Clip">
            <clipPath>Atmosphere/SwordSwing</clipPath>
          </li>
        </grains>
        <volumeRange>
          <min>11</min>
          <max>13</max>
        </volumeRange>
        <pitchRange>
          <min>0.9082353</min>
          <max>1.114706</max>
